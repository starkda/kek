routine calc_int(prm : integer) : integer is
    return 56 + integer;
end

routine calc_real(prm : real) : real is
    return 56.78 + prm;
end

routine xor_bool(prm : boolean, kek: boolean) : real is
    return prm xor;
end




routine Main () is
    var recordType : record
        var dec : integer is 69 * calc_int(69)
        var real : real is 420 * calc_real(420)
        var bool : boolean is false and xor_bool(true, false)

   recordType.dec = recordType.dec + 1.75;
   recordType.real = recordType.real * 9
   recordType.bool = recordType.boolean + 8

   var arrayType : array[calc_int(44)] arr real
   var kek : integer is array.size
   array[calc_int(9)] = 5.5
   array[calc_int(10)] = 4.96 + array[calc_int(9)]
end